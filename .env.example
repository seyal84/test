# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/homeflow?schema=public"

# API Configuration
API_BASE_URL="http://localhost:3000"
NODE_ENV="development"
PORT=3000

# JWT Configuration
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# AWS Configuration
AWS_REGION="us-west-2"
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_S3_BUCKET="homeflow-documents"
AWS_S3_PUBLIC_BUCKET="homeflow-public"

# AI Services
OPENAI_API_KEY="your-openai-api-key"
ANTHROPIC_API_KEY="your-anthropic-api-key"

# Payment Processing
STRIPE_SECRET_KEY="sk_test_your-stripe-secret-key"
STRIPE_PUBLISHABLE_KEY="pk_test_your-stripe-publishable-key"
STRIPE_WEBHOOK_SECRET="whsec_your-webhook-secret"

# Email Services
SENDGRID_API_KEY="your-sendgrid-api-key"
SENDGRID_FROM_EMAIL="noreply@homeflow.com"
SENDGRID_FROM_NAME="HomeFlow"

# SMS Services
TWILIO_ACCOUNT_SID="your-twilio-account-sid"
TWILIO_AUTH_TOKEN="your-twilio-auth-token"
TWILIO_PHONE_NUMBER="+1234567890"

# Redis (for caching and queues)
REDIS_URL="redis://localhost:6379"

# External APIs
GOOGLE_MAPS_API_KEY="your-google-maps-api-key"
ZILLOW_API_KEY="your-zillow-api-key"
MLS_API_KEY="your-mls-api-key"

# DocuSign Integration
DOCUSIGN_INTEGRATION_KEY="your-docusign-integration-key"
DOCUSIGN_USER_ID="your-docusign-user-id"
DOCUSIGN_ACCOUNT_ID="your-docusign-account-id"
DOCUSIGN_PRIVATE_KEY="your-docusign-private-key"

# Security
ENCRYPTION_KEY="your-32-character-encryption-key"
RATE_LIMIT_TTL=900
RATE_LIMIT_LIMIT=100

# Webhooks
WEBHOOK_SECRET="your-webhook-secret-for-verification"

# Monitoring
SENTRY_DSN="your-sentry-dsn-for-error-tracking"

# Frontend URLs
FRONTEND_URL="http://localhost:5173"
ADMIN_URL="http://localhost:5174"

# AI Processing
AI_PROCESSING_TIMEOUT=300000
MAX_DOCUMENT_SIZE=50000000
ALLOWED_FILE_TYPES="pdf,doc,docx,jpg,jpeg,png,bmp,tiff,xlsx,csv"

# Market Data
MARKET_DATA_API_KEY="your-market-data-api-key"
SCHOOL_DATA_API_KEY="your-school-data-api-key"

# Development
DEBUG_MODE="true"
LOG_LEVEL="debug"